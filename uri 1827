#include <stdio.h>


int main(void) {
	int i, n, j, o, ic, jc;
	
	while(scanf("%d",&n)!=EOF) {
		o = n/3;
		ic = jc = n/2;
		printf("%d, %d",o,ic);
		for(i=0;i<n;i++) {
			for(j=0;j<n;j++) {
				if (i == j && ((i<o)||(i>n-o))) {
					printf("2");
				
				}
				else if (i + j == n-1 &&((i<o)||(i>n-o))) {
					printf("3");	
				}
				else if (i<o || i>n-o) {
					printf("0");
				}
				else if((i>o || i<n-o) &&(i != ic && j != jc)) {
					
				}
				else printf("4");
			}
			printf("\n");
		}
		printf("\n");
	}
	
	return 0;
}
